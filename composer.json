{
    "name" : "goetas/twital",
    "description" : "PHP templating engine that combines Twig and PHPTal power points",
    "authors" : [{
            "name" : "Asmir Mustafic",
            "email" : "goetas@gmail.com"
        }
    ],
    "keywords" : [
        "php",
        "xml",
        "templating",
        "twig"
    ],
    "homepage" : "https://github.com/goetas/twital",
    "license" : "MIT",
    "require" : {
        "masterminds/html5" : "^2.1.2",
        "twig/twig" : "^1.10||^2.0",
        "symfony/event-dispatcher" : "~2.0",
        "php" : ">=5.3.3"
    },
    "require-dev" : {
        "phpunit/phpunit" : "~4.0"
    },
    "autoload" : {
        "psr-0" : {
            "Goetas\\Twital" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-0" : {
            "Goetas\\Twital\\Tests" : "tests/"
        }
    },
    "extra" : {
        "branch-alias" : {
            "dev-master" : "0.1-dev"
        }
    }
}
